{
  "family": "clarity-backend",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "3072",
  "containerDefinitions": [
    {
      "name": "clarity-backend",
      "image": "124355672559.dkr.ecr.us-east-1.amazonaws.com/clarity-backend:latest",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PORT",
          "value": "8000"
        },
        {
          "name": "ENVIRONMENT",
          "value": "production"
        },
        {
          "name": "ENABLE_AUTH",
          "value": "true"
        },
        {
          "name": "ENABLE_SELF_SIGNUP",
          "value": "false"
        },
        {
          "name": "CORS_ALLOWED_ORIGINS",
          "value": "https://api.clarity.health,http://localhost:3000,http://localhost:8000,https://clarity.health,https://clarity.novamindnyc.com"
        },
        {
          "name": "CORS_ALLOW_CREDENTIALS",
          "value": "true"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "COGNITO_REGION",
          "value": "us-east-1"
        },
        {
          "name": "COGNITO_USER_POOL_ID",
          "value": "us-east-1_efXaR5EcP"
        },
        {
          "name": "COGNITO_CLIENT_ID",
          "value": "7sm7ckrkovg78b03n1595euc71"
        },
        {
          "name": "DYNAMODB_TABLE_NAME",
          "value": "clarity-health-data"
        },
        {
          "name": "DYNAMODB_INSIGHTS_TABLE",
          "value": "clarity-ai-insights"
        },
        {
          "name": "DYNAMODB_USERS_TABLE",
          "value": "clarity-users"
        },
        {
          "name": "DYNAMODB_SESSIONS_TABLE",
          "value": "clarity-user-sessions"
        },
        {
          "name": "S3_BUCKET_NAME",
          "value": "clarity-health-data-storage"
        },
        {
          "name": "SQS_QUEUE_NAME",
          "value": "clarity-insights-queue"
        },
        {
          "name": "SNS_TOPIC_ARN",
          "value": "arn:aws:sns:us-east-1:124355672559:clarity-insights-topic"
        },
        {
          "name": "LOG_LEVEL",
          "value": "INFO"
        },
        {
          "name": "SKIP_EXTERNAL_SERVICES",
          "value": "false"
        },
        {
          "name": "S3_ML_MODELS_BUCKET",
          "value": "clarity-ml-models-124355672559"
        },
        {
          "name": "PAT_MODEL_BASE_DIR",
          "value": "/app/models/pat"
        },
        {
          "name": "PAT_S_MODEL_PATH",
          "value": "s3://clarity-ml-models-124355672559/pat/PAT-S_29k_weights.h5"
        },
        {
          "name": "PAT_M_MODEL_PATH",
          "value": "s3://clarity-ml-models-124355672559/pat/PAT-M_29k_weights.h5"
        },
        {
          "name": "PAT_L_MODEL_PATH",
          "value": "s3://clarity-ml-models-124355672559/pat/PAT-L_29k_weights.h5"
        },
        {
          "name": "PAT_S_CHECKSUM",
          "value": "37b3c530b8d9d89416d28fddbb4545de568cc52ddf6f5c700eeb4aa78f1c3616"
        },
        {
          "name": "PAT_M_CHECKSUM",
          "value": "c432f3f6f48cdd7817368d30fcad87708d6994c61324ae64885bb9f871dba575"
        },
        {
          "name": "PAT_L_CHECKSUM",
          "value": "19f9ad1235bdbea2636b6b40c6425536d624450f0b97302b7440bf978c3ee04f"
        }
      ],
      "secrets": [
        {
          "name": "GEMINI_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:124355672559:secret:clarity/gemini-api-key-nGOumD"
        },
        {
          "name": "GEMINI__API_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:124355672559:secret:clarity/gemini-api-key-nGOumD"
        },
        {
          "name": "SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:124355672559:secret:clarity/secret-key-j00HLA"
        },
        {
          "name": "SECURITY__SECRET_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:124355672559:secret:clarity/secret-key-j00HLA"
        },
        {
          "name": "GOOGLE_APPLICATION_CREDENTIALS_JSON",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:124355672559:secret:clarity/gcp-service-account-TxDX9f"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/clarity-backend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8000/health || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3
      }
    }
  ],
  "executionRoleArn": "arn:aws:iam::124355672559:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::124355672559:role/clarity-backend-task-role"
}